{
  "name": "robot_overlord_website",
  "version": "2.0.0",
  "description": "Website for Robot Overlord Software Inc",
  "private": true,
  "main": "support/index.js",
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "clean": "rimraf static/dist",
    "build": "npm run clean && webpack --config ./webpack.config.js --progress --profile --colors",
    "watch": "npm run clean && webpack-dev-server --hot --inline --config webpack.config.js",
    "serve": "npm run build && serve static",
    "start": "npm run watch",
    "test": "pulp test"
  },
  "repository": {
    "type": "git",
    "url": "git://git@github.com:robot-overlord/robot-overlord.github.io.git"
  },
  "author": "Brooklyn Zelenka",
  "license": "MIT",
  "engines": {
    "node": "^6.0.0"
  },
  "dependencies": {
    "bower": "^1.7.9",
    "css-loader": "^0.28.4",
    "node-sass": "^4.5.3",
    "normalize-css": "^2.3.1",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "purescript": "^0.11.4",
    "purescript-psa": "^0.5.1",
    "purs-loader": "^3.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.6",
    "serve": "^5.1.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.5.4",
    "xhr2": "^0.1.3"
  },
  "devDependencies": {}
}
